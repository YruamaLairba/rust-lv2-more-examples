@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .

# Plugin type. As this plugin doesn't do anything useful, it doesn't have
# subtype
<urn:rust-lv2-more-examples:eg-preset-rs>
        a lv2:Plugin ;
# Associated project
        lv2:project <https://github.com/YruamaLairba/rust-lv2-more-examples> ;
# Plugin name
        doap:name "Example Worker (rust-lv2)" ;
        doap:license <http://opensource.org/licenses/isc> ;
# ports description
        lv2:port [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:portProperty lv2:toggled;
                lv2:index 0 ;
                lv2:symbol "param1" ;
                lv2:name "Param 1" ,
                        "Param 1"@fr ;
                lv2:default 0.0 ;
                lv2:minimum -1.0 ;
                lv2:maximum +1.0 ;
                lv2:scalePoint [
                        rdfs:label "off" ;
                        rdf:value -1
                ] , [
                        rdfs:label "on" ;
                        rdf:value +1
                ]
        ] , [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:portProperty lv2:toggled;
                lv2:index 0 ;
                lv2:symbol "param2" ;
                lv2:name "Param 2" ,
                        "Param 2"@fr ;
                lv2:default 0.0 ;
                lv2:minimum -1.0 ;
                lv2:maximum +1.0 ;
                lv2:scalePoint [
                        rdfs:label "off" ;
                        rdf:value -1
                ] , [
                        rdfs:label "on" ;
                        rdf:value +1
                ]
        ] , [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:portProperty lv2:toggled;
                lv2:index 0 ;
                lv2:symbol "param3" ;
                lv2:name "Param 3" ,
                        "Param 3"@fr ;
                lv2:default 0.0 ;
                lv2:minimum -1.0 ;
                lv2:maximum +1.0 ;
                lv2:scalePoint [
                        rdfs:label "off" ;
                        rdf:value -1
                ] , [
                        rdfs:label "on" ;
                        rdf:value +1
                ]
        ] , [
